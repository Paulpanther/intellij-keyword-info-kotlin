<idea-plugin>
    <id>com.paulmethfessel.intellij-keywords-info-kotlin</id>
    <name>Keywords Info for Kotlin</name>
    <vendor>Paul Methfessel</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.paulmethfessel.kik.ide.KeywordService" id="com.paulmethfessel.kik.ide.KeywordService"/>
        <documentationProvider implementation="com.paulmethfessel.kik.ide.KeywordDocumentationProvider" id="com.paulmethfessel.kik.ide.KeywordDocumentationProvider"/>
    </extensions>
</idea-plugin>
