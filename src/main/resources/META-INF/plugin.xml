<idea-plugin>
    <id>com.paulmethfessel.intellij-keyword-info-kotlin</id>
    <name>Keyword Info for Kotlin</name>
    <vendor>Paul Methfessel</vendor>
    <change-notes>Add info on hover for all Kotlin Keywords (as of 2021-08-08)</change-notes>

    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.kotlin</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="com.paulmethfessel.kik.KeywordService" id="com.paulmethfessel.kik.KeywordService"/>
        <documentationProvider implementation="com.paulmethfessel.kik.KeywordDocumentationProvider" id="com.paulmethfessel.kik.KeywordDocumentationProvider"/>
    </extensions>
</idea-plugin>
